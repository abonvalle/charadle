{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "wordle": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/wordle",
      "sourceRoot": "projects/wordle/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/wordle",
            "index": "projects/wordle/src/index.html",
            "main": "projects/wordle/src/main.ts",
            "polyfills": "projects/wordle/src/polyfills.ts",
            "tsConfig": "projects/wordle/src/tsconfig.app.json",
            "assets": [
              "projects/wordle/src/favicon.ico",
              "projects/wordle/src/php",
              {
                "glob": "**/*",
                "input": "projects/wordle/src/assets/assets-serie",
                "output": "/assets/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/wordle/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/wordle/src/environments/environment.ts",
                  "with": "projects/wordle/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "production-anime": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "assets": [
                "projects/wordle/src/favicon.ico",
                "projects/wordle/src/php",
                {
                  "glob": "**/*",
                  "input": "projects/wordle/src/assets/assets-anime",
                  "output": "/assets/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/wordle/src/environments/environment.ts",
                  "with": "projects/wordle/src/environments/environment.prod.anime.ts"
                },
                {
                  "replace": "themes.js",
                  "with": "themes.anime.js"
                },
                {
                  "replace": "tsconfig.json",
                  "with": "tsconfig.anime.json"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "development-anime": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "assets": [
                "projects/wordle/src/favicon.ico",
                "projects/wordle/src/php",
                {
                  "glob": "**/*",
                  "input": "projects/wordle/src/assets/assets-anime",
                  "output": "/assets/"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/wordle/src/environments/environment.ts",
                  "with": "projects/wordle/src/environments/environment.anime.ts"
                },
                {
                  "replace": "themes.js",
                  "with": "themes.anime.js"
                },
                {
                  "replace": "tsconfig.json",
                  "with": "tsconfig.anime.json"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production-anime": {
              "browserTarget": "wordle:build:production-anime"
            },
            "development-anime": {
              "browserTarget": "wordle:build:development-anime"
            },
            "production": {
              "browserTarget": "wordle:build:production"
            },
            "development": {
              "browserTarget": "wordle:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wordle:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wordle/src/test.ts",
            "polyfills": "projects/wordle/src/polyfills.ts",
            "tsConfig": "projects/wordle/src/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "projects/wordle/src/favicon.ico",
              "projects/wordle/src/php",
              "projects/wordle/src/assets/assets-serie"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/wordle/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}